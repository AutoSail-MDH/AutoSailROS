<launch>
    <env name="ROSCONSOLE_CONFIG_FILE" value="$(find autosail)/config/custom_rosconsole.conf"/>
    <node pkg="autosail" name="true_wind_node" type="true_wind_node.py"/>
    <include file="$(find autosail)/launch/path_planner.launch"/>
    <include file="$(find autosail)/launch/ctrl.launch"/>
    <include file="$(find autosail)/test/startup.launch"/>
    <node pkg="autosail" name="network_node" type="network_node.py"/>
    <node pkg="web_video_server" name="web_video_server" type="web_video_server"/>
    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch"/>
</launch>
